{
  "name": "nextjs-template",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext js,ts,tsx .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "cleandep": "rm -rf node_modules && pnpm -r exec rm -rf node_modules"
  },
  "dependencies": {
    "@nayhoo/ui": "^0.9.6",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@vanilla-extract/next-plugin": "^2.4.8",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@nayhoo/eslint-config": "^1.0.2",
    "@nayhoo/prettier-config": "^1.0.1",
    "@nayhoo/tsconfig": "^1.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vanilla-extract/next-plugin": "^2.4.8",
    "eslint": "~8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^14.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5"
  }
}
